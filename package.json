{
  "name": "bootstrap-app",
  "version": "1.0.0",
  "main": "dist/index.html",
  "author": "Faramaz Patrick",
  "license": "MIT",
  "scripts": {
    "build:prod:bundlebuddy": "npm run build:prod -- --env.addons=bundlebuddy",
    "build:prod:bundleanalyze": "npm run build:prod -- --env.addons=bundleanalyze",
    "build:prod": "npm run build -- --env.env=prod",
    "debug:prod": "npm run debug -- --env.env=prod",
    "build:watch:prod": "npm run build:watch -- --env.env=prod",
    "build:watch:dev": "npm run build:watch -- --env.env=dev",
    "build:watch": "npm run build -- --watch",
    "build:allAddons": "npm run build:prod -- --env.addons=bundlebuddy --env.addons=bundleanalyze",
    "test": "NODE_ENV=test jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "debug": "node --inspect --debug-brk ./node_modules/webpack/bin/webpack.js",
    "build": "rm -rf dist/ && node ./node_modules/webpack/bin/webpack.js",
    "dev": "npm run webpack-dev-server -- --env.env=dev",
    "webpack-dev-server": "webpack-dev-server --port=8000 --hot",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "format": "prettier --write --single-quote --print-width=120 --tab-width=2 \"src/**/*.{js,jsx}\" ",
    "clean": "npm run format && npm run lint",
    "flow": "node_modules/.bin/flow",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "init": "npm i && flow-typed install",
    "start": "./node_modules/.bin/serve ./dist",
    "webpack-defaults": "webpack-defaults"
  },
  "dependencies": {
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-transform-inline-environment-variables": "^0.1.1",
    "dotenv": "^4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "serve": "^6.0.6",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "6.24.1",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "2.1.1",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flow": "2.29.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "2.1.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.52.0",
    "flow-remove-types": "^1.2.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "mobx-react-devtools": "^4.2.15",
    "nock": "^9.0.14",
    "prettier": "^1.5.3",
    "style-loader": "^0.18.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "uglifyjs-webpack-plugin": "beta",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0",
    "webpack-pwa-manifest": "^3.1.7"
  }
}
